<p id="notice"><%= notice %></p>

<h1>Tag Investigation Groups</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Investigation group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_investigation_groups.each do |tag_investigation_group| %>
      <tr>
        <td><%= tag_investigation_group.tag %></td>
        <td><%= tag_investigation_group.investigation_group %></td>
        <td><%= link_to 'Show', tag_investigation_group %></td>
        <td><%= link_to 'Edit', edit_tag_investigation_group_path(tag_investigation_group) %></td>
        <td><%= link_to 'Destroy', tag_investigation_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag Investigation Group', new_tag_investigation_group_path %>
