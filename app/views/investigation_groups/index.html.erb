<p id="notice"><%= notice %></p>

<h1>Investigation Groups</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Createdate</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @investigation_groups.each do |investigation_group| %>
      <tr>
        <td><%= investigation_group.name %></td>
        <td><%= investigation_group.createDate %></td>
        <td><%= investigation_group.description %></td>
        <td><%= link_to 'Show', investigation_group %></td>
        <td><%= link_to 'Edit', edit_investigation_group_path(investigation_group) %></td>
        <td><%= link_to 'Destroy', investigation_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Investigation Group', new_investigation_group_path %>
